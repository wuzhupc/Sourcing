<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wuzhupc.Sourcing"
    android:versionCode="121120"
    android:versionName="0.9.1120" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.WuzhuLight" >
        <activity
            android:name=".WelcomeActivity"
            android:theme="@style/Theme.WuzhuLightFullScreen"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:theme="@style/Theme.WuzhuLight" android:name="HomeActivity"></activity>
        
    </application>

</manifest>